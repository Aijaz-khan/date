<% if params[:mailbox] == "sent" %>
		<%= render :partial => "sent" %>
<% elsif params[:mailbox] == "inbox" %>
		<%= render :partial => "inbox" %>
<% elsif params[:mailbox] == "archieved" %>
        <%= render :partial => "inbox" %>
<% elsif params[:mailbox] == "unread" %>
		<%= render :partial => "inbox" %>
<% elsif params[:mailbox] == "trash" %>
		<%= render :partial => "trash" %>
<% end %>
