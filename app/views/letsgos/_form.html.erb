<%= form_for(@letsgo) do |f| %>
	<div class="form-group">
		<%= f.text_area :content, placeholder: "Propose new date...",class: 'form-control', style: 'height:25px;' %>
	</div>
	<div class="form-group" style="width:230px; text-align:left;">
		<%= f.select :tag, options_for_select( [["Select One", ""], "Eat/Drink", "Listen/Watch", "Play", "Explore", "Other"]), class: 'form-control' %>
	</div>
		<%= f.submit "Post", class: 'btn btn-primary', style: "margin: -112px -100px 5px 5px;" %>
		<% end %>
