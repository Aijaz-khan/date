<%= form_for @message, :url => user_messages_path(@user) do |f| %>
    <p>
      To:<br />
      <%= f.text_field :user_id, :value=>@letsgo.user_id,:readonly => true %>
    </p>
  <% end %>
      <p>
        Subject:<br />
        <%= f.text_field :subject, => "#{@messages.sender_id} asked you out on a date." %>
      </p>
      <p>
        Message<br />
        <%= f.text_area :body %>
      </p>

      <%= submit_tag "Send"%>
<% end %>
